{
    "name": "adiliogobira/laravel-breadcrumbs",
    "description": "A simple Laravel-style way to create breadcrumbs.",
    "keywords": [
        "laravel"
    ],
    "homepage": "https://github.com/adiliogobira/laravel-breadcrumbs",
    "authors": [
        {
            "name": "Adilio Gobira",
            "email": "adiliogobira@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "facade/ignition-contracts": "^1.0",
        "laravel/framework": ">=5.6"
    },
    "require-dev": {
        "orchestra/testbench": ">=3.6",
        "phpunit/phpunit": "^7.0|^8.0",
        "php-coveralls/php-coveralls": "^2.0",
        "spatie/phpunit-snapshot-assertions": "^2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "AdilioGobira\\Breadcrumbs\\": "classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/",
            "BreadcrumbsTests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AdilioGobira\\Breadcrumbs\\BreadcrumbsServiceProvider"
            ],
            "aliases": {
                "Breadcrumbs": "AdilioGobira\\Breadcrumbs\\Facades\\Breadcrumbs"
            }
        }
    }
}
